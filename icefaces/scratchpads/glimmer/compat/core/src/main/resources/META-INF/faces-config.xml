<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
              version="2.0"
              metadata-complete="false">
    <ordering>
        <after>
            <name>ICEfaces core</name>
        </after>
    </ordering>
    <render-kit>
        <render-kit-id>xHTML_BASIC</render-kit-id>
        <render-kit-class>com.icesoft.faces.component.CompatDOMRenderKit</render-kit-class>
    </render-kit>
    <application>
        <view-handler>com.icesoft.faces.application.ExtrasSetup</view-handler>
    </application>
</faces-config>
