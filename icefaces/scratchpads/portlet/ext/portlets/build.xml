<?xml version="1.0"?>

<project name="portlets" basedir="." default="deploy">
	<import file="build-parent.xml" />

	<target name="compile">
		<antcall target="compile-module">
			<param name="module.name" value="timezone" />
		</antcall>
	</target>

	<target name="clean">
		<antcall target="clean-module">
			<param name="module.name" value="timezone" />
		</antcall>
	</target>
</project>
