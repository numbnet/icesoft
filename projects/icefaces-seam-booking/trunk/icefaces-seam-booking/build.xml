<?xml version="1.0"?>

<project name="Booking" default="deploy" basedir=".">

    <!-- Naming -->
    <property name="Name"                   value="Seam on JBoss ICEfaces Booking Example"/>
    <property name="example.name"           value="icefaces-booking"/>
	
    <!-- Overrides -->
    <property name="seam.ui.lib"            value="yes"/>
