<?xml version="1.0" encoding="ISO-8859-1"?>
<testDefinition>

    <!-- The purpose of this test is to connect to timezone3, interact once, then
         go away without disposing the view. Just be idle. --> 

    <client>
        <clientClass>org.icesoft.testclient.client.TimezonePostbackClient</clientClass>
        <testURL>http://localhost:8080/timezone3</testURL>
        <clientCount>3000</clientCount>
        <initialDelay>5000</initialDelay>
        <interRequestDelay>2000</interRequestDelay>
        <repeatCount>2</repeatCount>
	<branch>false</branch>
    </client>

</testDefinition>

