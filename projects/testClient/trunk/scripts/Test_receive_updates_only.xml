<?xml version="1.0" encoding="ISO-8859-1"?>
<testDefinition>
    <!-- This can be run against any application where ping (1 thread) and receive-updated-view
       followed by receive-updates (2nd thread) is applicable
     -->
    
    <client>
        <clientClass>org.icesoft.testclient.client.TestReceiveUpdatesOnlyClient</clientClass>
        <testURL>http://localhost:8080/timezone3</testURL>
        <clientCount>1</clientCount>
        <initialDelay>1000</initialDelay>
        <interRequestDelay>5000</interRequestDelay>
        <repeatCount>200</repeatCount>
	<branch>false</branch>
    </client>

</testDefinition>

