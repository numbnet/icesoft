<?xml version="1.0" encoding="ISO-8859-1"?>
<testDefinition>

     <!-- The purpose of this test is to connect to timezone3 and just do the
      standard receive update and ping loops
      --> 
    <client>
        <clientClass>org.icesoft.testclient.client.StandalonePingReceiveUpdatesClient</clientClass>
        <testURL>http://localhost:8080/timezone3</testURL>
        <clientCount>200</clientCount>
        <initialDelay>5000</initialDelay>
        <interRequestDelay>200</interRequestDelay>
        <repeatCount>10</repeatCount>
        <branch>false</branch>
    </client>


</testDefinition>

