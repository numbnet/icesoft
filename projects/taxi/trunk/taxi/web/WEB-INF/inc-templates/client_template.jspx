<h:head>
    <meta http-equiv="Content-Type"
          content="text/html; charset=utf-8"></meta>
    <meta name="viewport" content="width=320; initial-scale=1.0; maximum-scale=1.0;" />
    <title>
        ICEfaces Taxi Demo - Client
    </title> 
    <ice:outputStyle href="css/mobile_ajax.css" /> 
</h:head>
<h:body>
<ice:form>

<div class="clientContainer">

    <!-- header-->
    <div class="smHeaderMobile">
        <!--<ice:commandLink actionListener="#{mediator.logout}">-->
            <div class="headerLogo" />
        <!--</ice:commandLink>-->
    </div>
    <div class="headerTile" />

    <!-- body -->
    <div class="smBodyMobile">
        <div>
            <!-- Page content insert -->
            <ui:insert name="pageContent">
                Default client template page body content.
            </ui:insert>
        </div>
    </div>

    <!-- footer-->
    <div class="headerTile" />
    <div class="smFooterMobile">
         <ui:insert name="pageFooter" />
    </div>


</div>
</ice:form>
    
</h:body>
</html>
