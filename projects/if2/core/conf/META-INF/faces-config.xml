    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
    version="2.0"
    metadata-complete="false">

<!--    <ordering><after><others/></after></ordering>  -->
    <factory>
        <partial-view-context-factory>org.icefaces.context.PartialViewContextFactory</partial-view-context-factory>
    </factory>
    <render-kit>
        <render-kit-id>xHTML_BASIC</render-kit-id>
        <render-kit-class>org.icefaces.render.DOMRenderKit</render-kit-class>
    </render-kit>
</faces-config>
