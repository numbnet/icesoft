<project name="JAXB-tree" default="build.examples">

    <description>
        Builds child projects 
    </description>
	
    <property name="build-common.xml" value="../../../../tutorials/build-common.xml"/>


    <!--
        builds the examples
    -->
    <target name="build.examples" >

        <subant target="copy.zip.src">
            <property name="build-common.xml" value="${build-common.xml}"/>
            <fileset dir="." includes="*/build.xml" />
        </subant>
        
    </target>

    <!--
       Cleans only the copied wars from build.examples.
    -->
    <target name="clean">
        <subant target="clean">
            <property name="build-common.xml" value="${build-common.xml}"/>
            <fileset dir="." includes="*/build.xml" />
        </subant>
    </target>

</project>

