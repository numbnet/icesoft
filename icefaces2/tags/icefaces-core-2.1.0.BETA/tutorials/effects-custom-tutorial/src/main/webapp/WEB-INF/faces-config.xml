<faces-config xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.0">
    <application xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
        <locale-config>
            <default-locale>en</default-locale>
        </locale-config>
    </application>

    <!-- Custom Panel Series example bean-->
    <managed-bean xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
        <description>
            Backing bean for custom effects example.
        </description>
        <managed-bean-name>customEffects</managed-bean-name>
        <managed-bean-class>
            com.icesoft.icefaces.tutorial.component.effects.custom.CustomEffects
        </managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>

</faces-config>