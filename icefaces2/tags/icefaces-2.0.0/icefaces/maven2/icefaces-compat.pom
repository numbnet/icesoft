  <description>ICEfaces Components and ICEfaces 1.8.x Compatibility APIs.</description> 
  <dependencies>
    <dependency>
      <groupId>org.icefaces</groupId>
      <artifactId>icefaces</artifactId>
      <version>2.0.0</version>
    </dependency>       

    <dependency>
      <groupId>commons-beanutils</groupId>
