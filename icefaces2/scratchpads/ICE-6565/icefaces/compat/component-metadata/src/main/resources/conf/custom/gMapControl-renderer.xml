<renderer>
	<description>
        <![CDATA[  
        This component adds the control to the ice:gMap. This component must need to be a child of ice:gMap component. (e.g.)<br/>
            &lt;ice:gMap&gt;<br/>
                &lt;ice:gMapControl name="GLargeMapControl"/&gt;<br/> 
               &lt;ice:gMapControl name="GScaleControl"/&gt;<br/> 
           &lt;/ice:gMap&gt;<br/>      
        ]]>
	</description>
	<component-family>com.icesoft.faces.GMapControl</component-family>

	<renderer-type>null</renderer-type>

	<renderer-extension>
		<instance-name>gMapControl</instance-name>
		<is-container>true</is-container>
		<renders-children>true</renders-children>
		<tag-name>gMapControl</tag-name>
		<taglib-prefix>@taglib-prefix@</taglib-prefix>
		<taglib-uri>@taglib-uri@</taglib-uri>
	</renderer-extension>

</renderer>
