<faces-config xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.0">
    <application>
        <locale-config>
            <default-locale>en</default-locale>
        </locale-config>
    </application>

    <!-- Basic OutputChart example bean-->
    <managed-bean>
        <description>
            Backing bean for outputChart example.
        </description>
        <managed-bean-name>axisChartBean</managed-bean-name>
        <managed-bean-class>
            com.icesoft.icefaces.tutorial.component.outputchart.basic.AxisChartBean
        </managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>

</faces-config>