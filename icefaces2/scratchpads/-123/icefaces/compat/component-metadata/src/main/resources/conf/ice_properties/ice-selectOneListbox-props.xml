&uiselectone-props;
&select-common-props;
&ice-att-partialSubmitDelay-props;
<property>
    <description>
        <![CDATA[
         <p>The base name for all style classes. Default is 
         <b>iceSelOneLb</b> if enabled and <b>iceSelOneLb-dis</b> 
         if disabled.
         </p>
		]]>
    </description>
    <property-name>styleClass</property-name>
    <property-class>java.lang.String</property-class>
    <property-extension>
        <category>ADVANCED</category>
        <editor-class>
           &styleClassEditor;
        </editor-class>
    </property-extension>
</property>

<property>
	<description>
		Number of available options to be shown at all times. If not specified, 
		all available options are shown.
	</description>  
    <property-name>size</property-name>
    <property-class>int</property-class>
    <property-extension>
        <category>JAVASCRIPT</category>
        <editor-class>
            &javascriptEditor;
        </editor-class>
    </property-extension>
</property>
       