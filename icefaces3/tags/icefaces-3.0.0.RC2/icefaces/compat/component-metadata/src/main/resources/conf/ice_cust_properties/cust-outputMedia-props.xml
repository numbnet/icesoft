&base-props;
&core-props;
&ice-att-userrole-rendered-props;
<property>
    <description>
        URL of the media data object. Relative URL is relative to the current web page.
    </description>
    <property-name>source</property-name>
    <property-extension>
        <category>ICE_COMPONENT</category>
        <editor-class>
            &stringEditor;
        </editor-class>
    </property-extension>
</property>
<property>
    <description>
        Message displayed while loading the media data.
    </description>
    <property-name>standbyText</property-name>
    <property-extension>
        <category>ICE_COMPONENT</category>
        <editor-class>
            &stringEditor;
        </editor-class>
    </property-extension>
</property>
<property>
    <description>
        Overrides default codebase set by the player attribute.
    </description>
    <property-name>codebase</property-name>
    <property-extension>
        <category>ICE_COMPONENT</category>
        <editor-class>
            &stringEditor;
        </editor-class>
    </property-extension>
</property>
<property>
    <description>
        MIME type of the media data. If not specified, the browser will try to determine
        the MIME type based on the media source file extension.
    </description>
    <property-name>mimeType</property-name>
    <property-extension>
        <category>ICE_COMPONENT</category>
        <editor-class>
             &stringEditor;
        </editor-class>
    </property-extension>
</property>
<property>
    <description>
        "windows", "flash", "quicktime" or "real". This sets the default values for the
        object tag's classid and codebase attributes and param name used to specify
        the URL of the media data object. It also sets the default value for the embed
        tag's pluginspage attribute.
    </description>
    <property-name>player</property-name>
    <property-extension>
        <category>ICE_COMPONENT</category>
        <editor-class>
            &selectOneDomainEditor;
        </editor-class>
    </property-extension>
</property>
