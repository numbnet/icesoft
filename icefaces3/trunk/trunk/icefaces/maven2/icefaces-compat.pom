 			<artifactId>servlet-api</artifactId> 
		 </exclusion> 
      </exclusions>
    </dependency>
        
    <dependency>
      <groupId>javax.el</groupId>
      <artifactId>el-api</artifactId>
      <version>1.0</version>
      <scope>provided</scope>
    </dependency>
