<?xml version="1.0" encoding="UTF-8"?>
<taglib>
<tlibversion>1.0</tlibversion>
<jspversion>1.1</jspversion>
<shortname>icepush</shortname>
<info>ICEpush JSP 1.1 Tags</info>
<uri>http://icepush.org</uri>
<tag>
	<name>register</name>
	<tagclass>org.icepush.integration.jsp.core.RegisterTag</tagclass>
	<info>ice.push.register</info>
	<attribute>
		<name>group</name>
		<required>false</required>
		<rtexprvalue>true</rtexprvalue>
	</attribute>
	<attribute>
		<name>notifier</name>
		<required>false</required>
	</attribute>
	<attribute>
		<name>callback</name>
		<required>true</required>
	</attribute>
</tag>
<tag>
	<name>region</name>
	<tagclass>org.icepush.integration.jsp.core.RegionTag</tagclass>
	<info>icepush.region</info>
	<attribute>
		<name>id</name>
		<required>false</required>
		<rtexprvalue>true</rtexprvalue>
	</attribute>
	<attribute>
		<name>group</name>
		<required>false</required>
		<rtexprvalue>true</rtexprvalue>
	</attribute>
	<attribute>
		<name>notifier</name>
		<required>false</required>
		<rtexprvalue>true</rtexprvalue>
	</attribute>
	<attribute>
		<name>page</name>
		<required>true</required>
		<rtexprvalue>true</rtexprvalue>
	</attribute>
</tag>
<tag>
	<name>pushPeriodic</name>
	<tagclass>org.icepush.integration.jsp.core.PushPeriodicTag</tagclass>
	<info>ice.push.pushPeriodic</info>
	<attribute>
		<name>group</name>
		<required>true</required>
		<rtexprvalue>true</rtexprvalue>
	</attribute>
	<attribute>
		<name>interval</name>
		<required>true</required>
		<type>java.lang.Long</type>
	</attribute>
</tag>
<tag>
	<name>push</name>
	<tagclass>org.icepush.integration.jsp.core.PushTag</tagclass>
	<info>ice.push.push</info>
	<attribute>
		<name>group</name>
		<required>true</required>
		<rtexprvalue>true</rtexprvalue>
	</attribute>
</tag>
</taglib>
